    "psr-4": {
      "Codeception\\Module\\": "src"
    }
  },
  "require": {
    "php"                    : "^7",
    "codeception/codeception": "~2"
  },
  "support": {
    "issues": "https://github.com/davidjeddy/codeception-hello-module/issues?state=open",
